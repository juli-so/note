    -ms-animation: shine 1.5s .2s;
    -o-animation: shine 1.5s .2s;
    animation: shine 1.5s .2s;
}
@-webkit-keyframes shine {
    25%{background-color:rgb(255,242,202);}
    50%{background-color:rgb(255,255,255);}
    75%{background-color:rgb(255,242,202);}
}
@-moz-keyframes shine {
    25%{background-color:rgb(255,242,202);}
    50%{background-color:rgb(255,255,255);}
    75%{background-color:rgb(255,242,202);}
}
@-ms-keyframes shine {
    25%{background-color:rgb(255,242,202);}
    50%{background-color:rgb(255,255,255);}
    75%{background-color:rgb(255,242,202);}
}
@-o-keyframes shine {
    25%{background-color:rgb(255,242,202);}
    50%{background-color:rgb(255,255,255);}
    75%{background-color:rgb(255,242,202);}
}
@keyframes shine {
    25%{background-color:rgb(255,242,202);}
    50%{background-color:rgb(255,255,255);}
    75%{background-color:rgb(255,242,202);}
}
